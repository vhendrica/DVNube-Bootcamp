Inicio

Escreva("Digite um número);
leia(n);
se (n > 0)
{
  Fibonacci(n);
}
senao
{
  Escreva("O número precisa ser positivo");
}



funcao Fibonacci(Inteiro n)
{
	se (n == 1)
	{
      retorne 0;
  }
  senao
  {
      se (n == 2)
      {
      retorne 1;
      }

      senao
      {
      retorne fibonacci(n-1) + fibonacci(n-2);
      }
	 }
}

Fim
