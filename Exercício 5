Inicio

Escreva("Digite um n√∫mero);
leia(n);
Fibonacci(n);

funcao Inteiro Fibonacci(Inteiro n)
{
	se (n < 0)
	{
		Escreva("Insira um valor positivo");
		retorne nulo;
	}
	se (n == 0)
	{
		retorne 0;
	}
	else
	{
		se (n == 1)
		{
		retorne 1;
		}
		
		else
		{
		retorne Fibonacci(n-1) + Fibonacci(n-2);
		}
	}
}

Fim
